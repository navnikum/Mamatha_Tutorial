{
  "root": "navigateToParentPage",
  "description": "Go to parent page",
  "actions": {
    "navigateToParentPage": {
      "module": "vb/action/builtin/navigateToPageAction",
      "parameters": {
        "page": "{{ $chain.variables.parentPage }}"
      }
    }
  },
  "variables": {
    "parentPage": {
      "type": "string",
      "required": true,
      "input": "fromCaller"
    }
  }
}